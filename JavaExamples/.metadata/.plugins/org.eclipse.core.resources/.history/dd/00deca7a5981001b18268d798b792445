package com.day3;

class Box{
	private double width;
	private double height;
	private double depth;
	
	Box(){
		width=-1;
		height=-1;
		depth=-1;
	}
	Box(double w, double h, double d){
		width=w;
		height=h;
		depth=d;
	}
	Box(int len){
		width=height=depth=len;
	}
	Box(Box ob){
		width=ob.width;
		height=ob.height;
		depth=ob.depth;
	}
	
	double volume() {
		return width*height*depth;
	}
}

public class Example1 {

	public static void main(String[] args) {
		

	}

}
