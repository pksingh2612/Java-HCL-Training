//H. Compilation fails.

package com.handon4;

class Clidders {

  public final void flipper() {

      System.out.println("Clidder");

  }

}

public class Clidlets {

	public void flipper() {
		
		        System.out.println("Flip a Clidlet");
		
		        super.flipper();
		
		    }   
		
		    public static void main(String[] args) {
		
		        new Clidlets().flipper();
		
		    }   

}
